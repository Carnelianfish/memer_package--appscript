function onOpen() {
  var ui = DocumentApp.getUi();
  var br = ui.createMenu('Big Red Button')
    .addItem('Customizable Launch', 'customizable_Launch')
    .addItem('Crash', 'crash_Prompt')
  var ig = ui.createMenu('Images&Gifs')
    .addItem('Bingus', 'bingus_A')
    .addItem('Amogus', 'amogus_A')
    .addItem('Yodabird', 'yoda_A')
    .addItem('OIIAII Cat', 'oiiaii_A')
    .addItem('Maxwell', 'maxwell_A')
    .addItem('Amogus?', 'amongtk_A')
    .addItem('Seal', 'seal_A')
  var cp = ui.createMenu('Copypastas')
    .addItem('Caught you in 8k', 'atek_Meme')
    .addItem('Fire', 'fire_Meme')
    .addItem('Mr Krabs', 'krabs_Meme')
    .addItem('The bee movie ', 'bee_Meme')
    ui.createMenu('Memer Package')
    .addSubMenu(br)
    .addSeparator()
    .addSubMenu(ig)
    .addSubMenu(cp)
    .addToUi()
}

