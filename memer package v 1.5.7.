function add_Image(url) {
  const cur = DocumentApp.getActiveDocument().getCursor();
  if(cur){
    var blob = UrlFetchApp.fetch(url).getBlob();
    cur.insertInlineImage(blob);
  }
}

function crash_Prompt() {
  var ui = DocumentApp.getUi();
  var result = ui.prompt('Really?', 'This will crash your doc', ui.ButtonSet.OK_CANCEL);

  if (result.getSelectedButton() == ui.Button.OK) {
    crasher()
  } else {
    Logger.log('User cancelled prompt.');
  }
}

function customizable_Launch() {
  var ui = DocumentApp.getUi();
  var response = ui.prompt('How many pages to fill', 'Input:', ui.ButtonSet.OK_CANCEL);

  if (response.getSelectedButton() == ui.Button.OK) {
    var pages = response.getResponseText();
    atomic(pages)
  } else {
    Logger.log('User cancelled prompt.');
  }
}

function crasher() {
  for (let counter = 1; counter <= 100; counter = counter + 1)
  DocumentApp.getActiveDocument().getBody().appendParagraph('')



}

function atomic(pages) {
  for (let counter = 1; counter <= pages; counter = counter + 1) {
  DocumentApp.getActiveDocument().getBody().appendParagraph(''); 
    }
}
